{
  "name": "willistowerswatson-automation",
  "version": "0.0.1",
  "description": "Willistowerswatson Automated BDD framework that uses webdriver.io, JavaScript and cucumber-js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "test/config/env/dev/dev.conf.js",
  "scripts": {
    "postinstall": "selenium-standalone install",
    "dev": "wdio test/config/env/dev/dev.conf.js",
    "selenium": "npm run removeReports && selenium-standalone start",
    "removeReports": "node ./test/config/bin/removeReports.js",
    "report": "allure serve test/reports/allure-results"
  },
  "keywords": [
    "test",
    "automation",
    "selenium",
    "webdriver",
    "webdriverIO"
  ],
  "dependencies": {
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "chromedriver": "^2.45.0",
    "commander": "^2.15.1",
    "cucumber": "^0.10.4",
    "deepmerge": "^2.1.1",
    "interval-promise": "^1.2.0",
    "lodash": "^4.17.13",
    "moment": "^2.22.2",
    "wait-until": "0.0.2",
    "wdio-cucumber-framework": "0.1.0",
    "wdio-dot-reporter": "0.0.5",
    "wdio-sauce-service": "^0.4.10",
    "wdio-screenshots-cleanup-service": "0.0.7",
    "webdriverio": "4.12.0"
  },
  "devDependencies": {
    "selenium-standalone": "^6.14.0",
    "wdio-allure-reporter": "^0.6.3",
    "wdio-appium-service": "^0.2.3",
    "wdio-browserstack-service": "^0.1.16",
    "wdio-chromedriver-service": "^0.1.5",
    "wdio-json-reporter": "^0.3.1",
    "wdio-junit-reporter": "0.0.2",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.4"
  }
}
